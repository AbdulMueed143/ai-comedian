ALTER TABLE company ADD COLUMN created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP, 
                       ADD COLUMN updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP;

ALTER TABLE companyhistory ADD COLUMN created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP, 
                             ADD COLUMN updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP;
ALTER TABLE graze ADD COLUMN created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP, 
                    ADD COLUMN updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP;
ALTER TABLE grazepaddock ADD COLUMN created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP, 
