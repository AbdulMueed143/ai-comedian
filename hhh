ALTER TABLE monthly_summary ADD COLUMN created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP, 
                              ADD COLUMN updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP;




ALTER TABLE paddock ADD COLUMN created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP, 
                      ADD COLUMN updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP;
ALTER TABLE paddock_to_current_paddock ADD COLUMN created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP, 
